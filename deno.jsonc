{
  "license": "Apache-2.0",
  "version": "0.0.1",
  "exports": "./src/mod.ts",
  "compilerOptions": {
    "lib": [
      "deno.ns",
      "dom",
      "deno.unstable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "npm:@types/react@^19.1.1"
  },
  "tasks": {
    "8check": "DENO_V8_FLAGS=--max-old-space-size=8192 deno check .",
    "ci": "deno fmt --check && deno lint && deno test -A --parallel --v8-flags=--max-old-space-size=8192 src/dataframe examples/dataframe tests/bugs tests/glm tests/statistical_tests --quiet",
    "ci:update-badge": "deno task ci && deno run --allow-read --allow-write --allow-run scripts/update-coverage-badge.ts",
    "benchmark:runner": "cd tests/benchmarks && deno run -A runner.ts && cd ../..",
    "benchmark:analyze": "cd tests/benchmarks && deno run -A analyze.ts && cd ../..",
    "benchmark": "deno task benchmark:runner && deno task benchmark:analyze",
    "benchmark-python": "cd tests/benchmarks && .venv/bin/python python.py && cd ../..",
    "benchmark-r": "cd tests/benchmarks && Rscript r.R && cd ../..",
    "benchmark-ts": "deno run -A tests/benchmarks/typescript.ts",
    "benchmark-tidy": "deno run -A tests/benchmarks/tidy-ts-only.ts",
    "benchmark-tidy-p": "deno run -A tests/benchmarks/tidy-ts-only-parallel.ts",
    "benchmark-rust-distinct": "cargo run --release --bin test_ultra_optimized_distinct",
    "test-src": "deno test -A --parallel --v8-flags=--max-old-space-size=8192 src/dataframe",
    "test-examples": "deno test -A --parallel --v8-flags=--max-old-space-size=8192 examples/dataframe",
    "test-stats-tests": "deno test -A --parallel --v8-flags=--max-old-space-size=8192 tests/statistical_tests/",
    "test-glm": "deno test -A --parallel --v8-flags=--max-old-space-size=8192 tests/glm/",
    "test-gee": "deno test -A --parallel --v8-flags=--max-old-space-size=8192 tests/gee/",
    "test-runtimes": "cd tests/runtimes && bash test-runner.sh",
    "wasmbuild": "deno run -A jsr:@deno/wasmbuild@0.19.2 --features wasm --out src/dataframe/lib",
    "docs:prerender": "cd docs && bun run prerender && cd ..",
    "joss:local-render": "devkit md2pdf tmp/proposal/draft-v3.md",
    "joss:render": "cd tmp/proposal && cp draft-v3.md paper.md && cp ../../paper.bib . && docker run --rm --volume \"$PWD:/data\" --user $(id -u):$(id -g) --env JOURNAL=joss openjournals/inara",
    "precommit": "deno task ci:update-badge && deno task docs:prerender"
  },
  "importMap": "import_map.json",
  "nodeModulesDir": "auto",
  "vendor": true,
  "fmt": {
    "exclude": [
      "node_modules/",
      "LICENSE",
      "**/*.md",
      "**/*.mdc",
      "**/*.ipynb",
      ".claude/",
      ".cargo/",
      "target/",
      "vendor/",
      "z-other/"
    ]
  },
  "exclude": [
    "node_modules",
    "vendor",
    "z-other",
    "sources",
    "examples/docs-tests",
    "sources/**",
    ".cargo/",
    "tests/runtimes/**",
    "target/",
    "docs/",
    "docs/**",
    "tidy-stats/",
    "src/tidy-stats/rust",
    "tests/tidy-stats/__pycache__",
    "tests/tidy-stats/.venv",
    "docs/src/routeTree.gen.ts"
  ],
  "workspace": [
    "src/dataframe"
  ]
}
